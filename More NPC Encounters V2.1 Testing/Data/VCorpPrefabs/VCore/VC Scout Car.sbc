if (battery.GetValueBool("Recharge")) 
prefix = "(+) "; 
else 
if (battery.GetValueBool("Discharge")) 
prefix = "(-) "; 
else 
prefix = "() "; 
return prefix + ((battery.CurrentStoredPower / battery.MaxStoredPower) * 100.0f).ToString("0.0") + "%"; 
} 
 
