blocks.AddBOfName(cmd.nameLike); 
 
if (blocks.Count() &lt;= 0) { 
MMLCDMgr.AddLn("Details: " + M.T["D2"]); 
return; 
} 
 
int si = (cmd.cmdstr.EndsWith("x") ? 1 : 0); 
