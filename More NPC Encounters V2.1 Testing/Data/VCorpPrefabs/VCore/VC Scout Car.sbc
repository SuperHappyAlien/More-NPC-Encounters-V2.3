MMLCDMgr.AddLn(block.CustomName); 
PrintDetails(block, si); 
 
for (int i = 1; i &lt; blocks.Count(); i++) { 
block = blocks.Blocks[i]; 
MMLCDMgr.AddLn(""); 
MMLCDMgr.AddLn(block.CustomName); 
PrintDetails(block, si); 
} 
} 
 
