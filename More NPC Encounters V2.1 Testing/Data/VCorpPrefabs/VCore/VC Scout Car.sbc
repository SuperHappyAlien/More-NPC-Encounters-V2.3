public static class MMLCDMgr 
{ 
private static Dictionary&lt;IMyTextPanel, MMLCDText&gt; texts = new Dictionary&lt;IMyTextPanel, MMLCDText&gt;(); 
public static bool forceRedraw = true; 
public static int SCROLL_LINES = 5; 
