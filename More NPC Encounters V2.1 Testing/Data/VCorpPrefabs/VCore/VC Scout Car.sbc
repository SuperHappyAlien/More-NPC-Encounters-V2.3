public void RunText(MMCmd cmd) 
{ 
bool fromLCD = (cmd.cmdstr == "textlcd"); 
 
IMyTextPanel p = MMLCDMgr.curP.first; 
if (p == null) 
return; 
 
string text = p.GetPrivateText(); 
 
