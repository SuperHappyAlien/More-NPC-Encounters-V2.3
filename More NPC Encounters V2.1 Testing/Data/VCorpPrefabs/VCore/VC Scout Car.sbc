} 
 
if (tankCnt == 0) { 
if (!found) 
MMLCDMgr.AddLn(M.T["O3"]); 
return; 
} 
 
percent = tankSum / tankCnt; 
 
if (found) 
MMLCDMgr.AddLn(""); 
MMLCDMgr.Add(M.T["O4"]); 
MMLCDMgr.AddRightLn(percent.ToString("F2") + "%", LCD_LINE_WORK_STATE_POS); 
