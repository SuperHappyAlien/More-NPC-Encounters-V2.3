{ 
public int min; 
public int max; 
public string subType = ""; 
public string mainType = ""; 
public bool ignore; 
public double current; 
 
public MMAmountSpec(bool _ignore = false, int _min = 1, int _max = -1) 
{ 
min = _min; 
