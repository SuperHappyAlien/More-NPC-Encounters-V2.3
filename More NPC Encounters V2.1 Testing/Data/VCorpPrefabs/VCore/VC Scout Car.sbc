return items[subType + " " + mainType]; 
 
if (mainType == "") { 
MMItem item = null; 
itemsBySubtype.TryGetValue(subType, out item); 
return item; 
} 
 
if (subType == "") 
for (int i = 0; i &lt; items.Count; i++) { 
MMItem item = items[keys[i]]; 
if (mainType == item.mainType) 
return item; 
} 
return null; 
} 
