if (missing &amp;&amp; num &gt;= items[i].min) 
continue; 
 
int quota = items[i].max; 
if (missing) 
quota = items[i].min; 
 
string msg = MM.ToDisplay(items[i].subType, items[i].mainType); 
 
ShowInvLine(msg, num, quota); 
} 
} 
