public void RunInvListing(MMCmd cmd) 
{ 
MMBlkCol blocks = new MMBlkCol(); 
bool noexpand = false; 
bool simple = false; 
if (cmd.cmdstr[cmd.cmdstr.Length - 1] == 's') { 
