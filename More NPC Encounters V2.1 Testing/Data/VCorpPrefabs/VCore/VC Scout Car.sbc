MMLCDMgr.AddLn("Charge: " + M.T["D2"]); 
return; 
} 
 
for (int i = 0; i &lt; blocks.Count(); i++) { 
IMyJumpDrive jd = blocks.Blocks[i] as IMyJumpDrive; 
 
double cur, max, perc; 
perc = MMStatus.GetJDChargeVals(jd, out cur, out max); 
 
