if (item.dName != "") 
return item.dName; 
 
return item.subType; 
} 
 
return System.Text.RegularExpressions.Regex.Replace( 
subType, "([a-z])([A-Z])", "$1 $2"); 
} 
 
public static void ToInternal(ref string subType, ref string mainType) 
{ 
