    string strRunningSymbol = ""; 
     
    if (runningSymbolVariant == 0) 
        strRunningSymbol = "|"; 
    else if (runningSymbolVariant == 1) 
        strRunningSymbol = "/"; 
    else if (runningSymbolVariant == 2) 
        strRunningSymbol = "--"; 
    else if (runningSymbolVariant == 3) 
    { 
        strRunningSymbol = "\\"; 
        runningSymbolVariant = -1; 
    } 
 
    return strRunningSymbol; 
} 
 
