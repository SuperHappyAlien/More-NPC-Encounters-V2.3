const string strGroupTag = "Turret Group"; //name tag of turret groups 
const string strDesignatorName = "Designator"; //name of the designator turret for specific group 
const string strElevationRotorName = "Elevation"; //name of elevation (vertical) rotor for specific turret 
const string strAzimuthRotorName = "Azimuth"; //name of azimuth (horiontal) rotor for specific turret 
 
bool autofireAllowed = true; //specifies whether or not the turret will fire without manual command 
 
const double timeToReset = 3; //specifies the amount of time before the code resets the turret elevation 
 
const double toleranceAngle = 5; //angle that the turret will fire on if target is within this angle from the front of it 
const double updatesPerSecond = 10; 
const double proportionalConst = 40; //controls the speed of rotation; you probably shouldn't touch this 
 
//////////////////////////////////////////////////// 
//================================================= 
//No touchey anything below here 
//================================================= 
//////////////////////////////////////////////////// 
 
const double timeMax = 1 / updatesPerSecond; 
 
double timeElapsed = 0; 
double timeIdle = 5; 
double azimuthAngle = 0; 
double elevationAngle = 0; 
double designatorAzimuth; 
double designatorElevation; 
bool isIdle = true; 
 
IMyMotorStator elevationRotor; 
IMyMotorStator azimuthRotor;  
IMyLargeTurretBase designator; 
Vector3D targetPointVec; 
Vector3D transformedTargetVec; 
List&lt;IMyTerminalBlock&gt; listWeaponsOrTools = new List&lt;IMyTerminalBlock&gt;(); //need to clear this 
 
const double rad2deg = 180 / Math.PI; 
 
const double minRange = 100; //minimum engagement range to sweep guns 
const double maxRange = 800; //maximum engagement range to sweep guns 
const double spreadFrequency = .7; //frequency of one half sweep 
double engagementRange = 0; 
double timeSpread = 0; 
 
void WeaponSweep() 
{ 
    if( timeSpread &lt; spreadFrequency ) 
    { 
        engagementRange = ( maxRange - minRange ) * timeSpread / spreadFrequency + minRange; 
    } 
    else if( timeSpread &lt; 2 * spreadFrequency ) 
    { 
        engagementRange = maxRange - ( maxRange - minRange ) * ( timeSpread - spreadFrequency ) / spreadFrequency ; 
    }else{ 
        timeSpread = 0; 
    } 
} 
 
void Main( string arg ) 
{ 
    switch( arg.ToLower() ) 
    {  
        case "autofire on": 
            autofireAllowed = true; 
            break; 
             
        case "autofire off": 
            autofireAllowed = false; 
            break; 
             
        case "autofire toggle": 
            if ( autofireAllowed == false ) //if false switch true    
            {    
                autofireAllowed = true; 
            }else{ //if true switch false    
                autofireAllowed = false; 
            }    
            break; 
             
        default: 
            break; 
    } 
 
    timeElapsed += Runtime.TimeSinceLastRun.TotalSeconds; 
    timeSpread += Runtime.TimeSinceLastRun.TotalSeconds; 
    trackSymbol += Runtime.TimeSinceLastRun.TotalSeconds; 
    timeIdle += Runtime.TimeSinceLastRun.TotalSeconds; 
 
    WeaponSweep(); //run weapon sweeep program 
 
    if(timeElapsed + 0.000001 &gt;= timeMax) 
    { 
        Echo( "WMI Turret Control Systems Online... " + RunningSymbol() ); 
         
        List&lt;IMyBlockGroup&gt; groups = new List&lt;IMyBlockGroup&gt;(); 
        List&lt;IMyBlockGroup&gt; turretGroups = new List&lt;IMyBlockGroup&gt;(); 
         
        GridTerminalSystem.GetBlockGroups(groups); 
         
        foreach (IMyBlockGroup thisGroup in groups) 
        { 
            if (thisGroup.Name.Contains(strGroupTag)) 
            { 
                turretGroups.Add(thisGroup); 
            } 
        } 
         
         
        Echo("\nTurret List:"); 
        if( turretGroups.Count == 0 ) 
        { 
            Echo("Error: No turret groups found!"); 
            return; 
        } 
         
         
        foreach (IMyBlockGroup thisGroup in turretGroups) 
        { 
            var blockList = new List &lt;IMyTerminalBlock&gt;(); 
            thisGroup.GetBlocks(blockList); 
            GrabBlocks(blockList); 
             
            //GrabBlocks( thisGroup.Blocks ); //old way that doesnt work anymore 
             
            bool setupError = false; 
            if( designator == null ) 
            { 
                Echo("Error: No designator turret found for group '" + thisGroup.Name + "'" ); 
                setupError = true; 
            } 
             
            if( listWeaponsOrTools.Count == 0 ) 
            { 
                Echo( "Error: No weapons for group '" + thisGroup.Name + "'" ); 
                setupError = true; 
            } 
             
            if( azimuthRotor == null ) 
            { 
                Echo( "Error: No azimuth rotor for group '" + thisGroup.Name + "'" ); 
                setupError = true; 
            } 
             
            if( elevationRotor == null ) 
            { 
                Echo( "Error: No elevation rotor for group '" + thisGroup.Name + "'" ); 
                setupError = true; 
            } 
 
            if( setupError ) 
            { 
                StopRotorMovement(); 
                designator = null; 
                azimuthRotor = null; 
                elevationRotor = null; 
                listWeaponsOrTools.Clear(); 
            } 
            else 
            { 
                if( timeIdle &gt; timeToReset ) 
                { 
                    ResetDesignatorPos(); 
                    StopRotorMovement(); 
                } 
                else 
                { 
                    //get target from designator 
                    GetTargetPoint(); 
                     
                    //guide on target 
                    RotorControl(); 
                } 
 
                listWeaponsOrTools.Clear(); //clear our lists 
                Echo( thisGroup.Name + " is online"); //write successful groups to  
            } 
        } 
 
        //reset time count 
        timeElapsed = 0; 
 
        if( timeIdle &gt; timeToReset ) 
        { 
            timeIdle = 0; 
        } 
        turretGroups.Clear(); 
    } 
     
    DisableIdleMovement(); 
} 
 
void GetTargetPoint() 
{ 
    //get designator position 
    Vector3D designatorPos = designator.GetPosition(); 
 
    //get designator orientation vectors 
    Vector3D designatorForwardVec = designator.WorldMatrix.Forward; 
    Vector3D designatorLeftVec = designator.WorldMatrix.Left; 
    Vector3D designatorUpVec = designator.WorldMatrix.Up; 
     
    //get vector where designator is pointing 
    //SyncTurretAngles( designator ); 
    designatorAzimuth = (double)designator.Azimuth; 
    designatorElevation = (double)designator.Elevation; 
     
    //convert azimuth and elevation to a useful vector 
    Vector3D targetVec = VectorAzimuthElevation( designatorAzimuth, designatorElevation ); 
 
    //get transformed target vector 
    transformedTargetVec = designatorForwardVec * targetVec.X + designatorLeftVec * targetVec.Y + designatorUpVec * targetVec.Z; 
    transformedTargetVec = Vector3D.Normalize( transformedTargetVec ); 
     
    //check if we are idle 
    isIdle = designatorElevation == 0; 
     
    targetPointVec = designatorPos + transformedTargetVec * engagementRange; 
} 
 
void GrabBlocks( List&lt;IMyTerminalBlock&gt; blocks ) 
{ 
    foreach(IMyTerminalBlock thisBlock in blocks) 
    { 
        if( thisBlock is IMyMotorStator ) 
        { 
            if( thisBlock.CustomName.Contains( strElevationRotorName ) ) 
            { 
                elevationRotor = thisBlock as IMyMotorStator; 
            } 
            else if( thisBlock.CustomName.Contains( strAzimuthRotorName ) ) 
            { 
                azimuthRotor = thisBlock as IMyMotorStator; 
            }else{ 
                continue; 
            } 
        } 
        else if( thisBlock is IMyUserControllableGun &amp;&amp; !( thisBlock is IMyLargeTurretBase ) ) 
        { 
            listWeaponsOrTools.Add( thisBlock ); //we only want to control out fixed guns 
        } 
        else if( thisBlock.CustomName.Contains( strDesignatorName ) &amp;&amp; thisBlock is IMyLargeTurretBase )  
        { 
            designator = thisBlock as IMyLargeTurretBase; //grabs designator turret 
        } 
        else if( thisBlock is IMyShipToolBase ) 
        { 
            listWeaponsOrTools.Add( thisBlock ); 
        } 
        else if( thisBlock is IMyLightingBlock ) 
        { 
            listWeaponsOrTools.Add( thisBlock ); 
        } 
        else if( thisBlock is IMyCameraBlock ) 
        { 
            listWeaponsOrTools.Add( thisBlock ); 
        } 
    } 
} 
 
void RotorControl() 
{ 
    //get orientation of reference 
    IMyTerminalBlock turretReference = listWeaponsOrTools[0]; 
    Vector3D turretFrontVec = turretReference.WorldMatrix.Forward;  
    Vector3D absUpVec = azimuthRotor.WorldMatrix.Up; 
    Vector3D turretSideVec = elevationRotor.WorldMatrix.Up; 
    Vector3D turretFrontCrossSide = turretFrontVec.Cross( turretSideVec ); 
     
    //check elevation rotor orientation w.r.t. reference 
    Vector3D turretUpVec; 
    Vector3D turretLeftVec; 
    if( DotIsSameDirection( absUpVec, turretFrontCrossSide ) ) 
    { 
        turretUpVec = turretFrontCrossSide; 
        turretLeftVec = turretSideVec; 
    }else{ 
        turretUpVec = -1 * turretFrontCrossSide; 
        turretLeftVec = -1 * turretSideVec; 
    } 
 
    //get vector to target point 
    Vector3D referenceToTargetVec = targetPointVec - turretReference.GetPosition(); 
 
    //get projections onto axis made out of our plane orientation 
    Vector3D projOnFront = VectorProjection( referenceToTargetVec, turretFrontVec ); 
    Vector3D projOnLeft = VectorProjection( referenceToTargetVec, turretLeftVec ); 
    Vector3D projOnUp = VectorProjection( referenceToTargetVec, turretUpVec ); 
    Vector3D projOnFrontLeftPlane = projOnFront + projOnLeft; 
     
    azimuthAngle = Math.Asin( projOnLeft.Length() * DotGetSign( projOnLeft, turretLeftVec ) / projOnFrontLeftPlane.Length() ); 
    elevationAngle = Math.Atan( projOnUp.Length() * DotGetSign( projOnUp, turretUpVec ) / projOnFrontLeftPlane.Length() );                                                                                                                                                                                                                                              //w.H*i/P=L-a!s,H-1,4.1 m.a/d.e t/h.i/s 
     
    if( DotIsSameDirection( absUpVec, turretFrontCrossSide ) ) 
    { 
        elevationAngle *= -1; 
    } 
 
    double azimuthSpeed = proportionalConst * azimuthAngle; //derivitave term is useless as rotors dampen by default 
    double elevationSpeed = proportionalConst * elevationAngle; 
     
    //check if idle 
    if( isIdle ) //this locks the turret if idle 
    { 
        azimuthSpeed = 0; elevationSpeed = 0; 
    } 
 
    //control rotors  
    azimuthRotor.SetValue( "Velocity", -(float)azimuthSpeed ); //negative because we want to cancel the positive angle via our movements 
    elevationRotor.SetValue( "Velocity", -(float)elevationSpeed ); 
     
    //calculate deviation angle 
    double deviationAngle = Math.Acos( turretFrontVec.Dot( referenceToTargetVec ) / turretFrontVec.Length() / referenceToTargetVec.Length()  ); 
    WeaponControl( deviationAngle ); 
} 
 
void WeaponControl( double deviation ) 
{ 
    var shootingDesignator = designator as IMyUserControllableGun; //trying to see if gun is shooting 
     
    /*Echo(designator.IsUnderControl.ToString()); 
    Echo(shootingDesignator.IsShooting.ToString());*/ 
     
    if(designator.IsUnderControl) 
    { 
    //fires if designator is controlled and shooting 
        if (shootingDesignator != null &amp;&amp; shootingDesignator.IsShooting) 
        {  
            for (int i = 0; i &lt; listWeaponsOrTools.Count; i++)     
            {     
                var weaponToShoot = listWeaponsOrTools[i] as IMyUserControllableGun; 
                if( weaponToShoot != null ) 
                { 
                    if( weaponToShoot.GetValue&lt;bool&gt;("Shoot") == false ) 
                        weaponToShoot.ApplyAction("Shoot_On"); 
                } 
            }  
        } 
    } 
    else if( deviation * rad2deg &lt; toleranceAngle &amp;&amp; designatorAzimuth != 0 &amp;&amp; designatorElevation != 0 &amp;&amp; autofireAllowed ) 
    { 
    //fires if in tolerance angle 
        for (int i = 0; i &lt; listWeaponsOrTools.Count; i++)     
        {     
            var weaponToShoot = listWeaponsOrTools[i] as IMyUserControllableGun; 
            if( weaponToShoot != null ) 
            {             
                if( weaponToShoot.GetValue&lt;bool&gt;("Shoot") == false ) 
                    weaponToShoot.ApplyAction("Shoot_On"); 
            } 
        } 
    }else{ 
    //fires if not in tolerance angle or designator isnt controlled 
        for (int i = 0; i &lt; listWeaponsOrTools.Count; i++)     
        {     
            var weaponToShoot = listWeaponsOrTools[i] as IMyUserControllableGun; 
            if( weaponToShoot != null ) 
            { 
                if( weaponToShoot.GetValue&lt;bool&gt;("Shoot") == true ) 
                    weaponToShoot.ApplyAction("Shoot_Off"); 
            } 
        }    
    } 
} 
 
//check if base is same dirn as turret 
 
double DotGetSign( Vector3D a, Vector3D b ) 
{ 
    double x = a.Dot( b ); 
    return x/Math.Abs(x); 
} 
 
bool DotIsSameDirection( Vector3D a, Vector3D b ) 
{ 
    double x = a.Dot( b ); 
    return Math.Abs(x) == x; 
} 
 
void SyncTurretAngles( IMyLargeTurretBase turret ) 
{ 
    turret.SyncAzimuth(); 
    turret.SyncElevation(); 
    turret.SyncEnableIdleRotation(); 
} 
 
void StopRotorMovement() 
{ 
    if (azimuthRotor != null) 
    { 
        azimuthRotor.SetValue( "Velocity", 0f ); 
    } 
     
    if (elevationRotor != null) 
    { 
        elevationRotor.SetValue( "Velocity", 0f ); 
    } 
     
    for (int i = 0; i &lt; listWeaponsOrTools.Count; i++) 
    { 
        var thisWeapon = listWeaponsOrTools[0] as IMyUserControllableGun; 
        if (thisWeapon != null) thisWeapon.ApplyAction("Shoot_Off"); 
    } 
} 
 
void ResetDesignatorPos() 
{ 
    //designator.Azimuth = 0;  
    //this was suggested to be removed by reddit user Ocean__Sunfish 
     
    //Modified reset idea from Rdav :) 
    if (designator != null) 
    { 
        designator.Elevation = 0;  
        designator.SyncAzimuth(); 
        designator.SyncElevation(); 
        designator.ResetTargetingToDefault(); 
    } 
} 
 
//This forces idle movement off (should fix behavior in MP) 
void DisableIdleMovement() 
{ 
    var allDesignators = new List &lt;IMyTerminalBlock&gt;(); 
    GridTerminalSystem.SearchBlocksOfName(strDesignatorName, allDesignators, IsTurret); 
    foreach (IMyLargeTurretBase thisDesignator in allDesignators) 
    { 
        if (thisDesignator.GetValue&lt;bool&gt;("EnableIdleMovement") == true) 
        { 
            thisDesignator.ApplyAction("EnableIdleMovement_Off"); 
        } 
 
        thisDesignator.SyncEnableIdleRotation(); 
    } 
} 
 
bool IsTurret(IMyTerminalBlock test_block) 
{ 
    var cast_turret = test_block as IMyLargeTurretBase; 
    return cast_turret != null; 
} 
 
Vector3D VectorProjection( Vector3D a, Vector3D b ) 
{ 
    Vector3D projection = a.Dot( b ) / b.Length() / b.Length() * b;   
    return projection; 
} 
 
//Whip's Vector from Elevation and Azimuth 
Vector3D VectorAzimuthElevation( double az, double el ) 
{ 
    el = el % ( 2 * Math.PI ); 
    az = az % ( 2 * Math.PI ); 
 
    if( az != Math.Abs( az ) ) 
    { 
        az = 2 * Math.PI + az; 
    } 
 
    int x_mult = 1; 
 
    if( az &gt; Math.PI / 2 &amp;&amp; az &lt; Math.PI ) 
    { 
        az = Math.PI - ( az % Math.PI ); 
        x_mult = -1; 
    } 
    else if ( az &gt; Math.PI &amp;&amp; az &lt; Math.PI * 3 / 2 ) 
    { 
        az = 2 * Math.PI - ( az % Math.PI ); 
        x_mult = -1; 
    } 
 
    double x; double y; double z; 
 
    if( el == Math.PI / 2 ) 
    { 
        x = 0; 
        y = 0; 
        z = 1; 
    } 
    else if ( az == Math.PI / 2 ) 
    { 
        x = 0; 
        y = 1; 
        z = y * Math.Tan( el ); 
    }else{ 
        x = 1 * x_mult; 
        y = Math.Tan( az ); 
        double v_xy = Math.Sqrt( 1 + y * y ); 
        z = v_xy * Math.Tan( el ); 
    } 
 
    return new Vector3D( x, y, z ); 
} 
 
//Whip's Running Symbol Method 
double trackSymbol = 0; 
string strRunningSymbol = ""; 
 
string RunningSymbol() 
{ 
    if(trackSymbol &lt; .2d) 
        strRunningSymbol = "|"; 
    else if(trackSymbol &lt; .4d) 
        strRunningSymbol = "/"; 
