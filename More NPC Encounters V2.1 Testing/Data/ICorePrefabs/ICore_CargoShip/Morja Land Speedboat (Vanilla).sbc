                xstart = 0;  
                for (var y = ystart; y &lt; dy; y++) {  
                    if (ASCS_shutdown) break;  
                    ystart = 0;  
                    for (var z = zstart; z &lt; dz; z++) {  
                        if (ASCS_cutoff &lt; program.Runtime.CurrentInstructionCount) {  
                            ASCS_shutdown = true;  
                            ASCS_ind = new int[] {x, y, z}; 
                            ASCS_grid = grid; 
                            break;  
                        }  
                        zstart = 0; 
